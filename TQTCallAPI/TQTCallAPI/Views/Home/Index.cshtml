@model  List<SSC.Core.Api.Models.Entity.UserEntity>
<h2>List User</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>User Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FullName</td>
                <td>@item.Username</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>


