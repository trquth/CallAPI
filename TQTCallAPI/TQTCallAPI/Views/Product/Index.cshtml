@model  List<TQTCallAPI.ViewModel.ProductViewModel>
<h2>List User</h2>
@if (Model != null)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Empty!!!!</p>
}

